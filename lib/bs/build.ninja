g_pkg_flg = -bs-package-name next-reason-app
src_root_dir = /home/book/Documents/WebDev_Practice/finalProject/next-reason-app
bsc = /home/book/Documents/WebDev_Practice/finalProject/next-reason-app/node_modules/bs-platform/linux/bsc.exe
bsdep = /home/book/Documents/WebDev_Practice/finalProject/next-reason-app/node_modules/bs-platform/linux/bsb_helper.exe
warnings = 
bsc_flags = -bs-super-errors
ppx_flags = 
g_dpkg_incls = 
g_ns = 
bsb_dir_group = 0
g_lib_incls = -I src -I /home/book/Documents/WebDev_Practice/finalProject/next-reason-app/node_modules/reason-react/lib/ocaml
rule build_ast_from_re
  command = $bsc  $warnings -color always -bs-jsx 3 $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/Next.reast : build_ast_from_re $src_root_dir/src/Next.re
rule build_deps
  command = $bsdep -hash e7760c9535065ee4ffa1d82aa760b47a $g_ns -g $bsb_dir_group $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/Next.d : build_deps src/Next.reast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg -color always -bs-suffix $g_lib_incls $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/Next.cmj |  src/Next.cmi $src_root_dir/src/Next.bs.js : ml_cmj_cmi src/Next.reast ||  src/Next.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:src
